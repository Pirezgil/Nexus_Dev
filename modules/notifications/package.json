{
  "name": "nexus-notifications",
  "version": "1.0.0",
  "description": "Sistema unificado de notificações para ERP Nexus",
  "main": "dist/src/app.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/app.ts",
    "start": "node dist/src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "ts-node prisma/seed.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "@prisma/client": "5.3.1",
    "axios": "^1.5.0",
    "bull": "^4.11.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase-admin": "^13.5.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mustache": "^4.2.0",
    "nodemailer": "^6.9.4",
    "rate-limiter-flexible": "^2.4.2",
    "socket.io": "^4.7.2",
    "twilio": "^4.18.0",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.197",
    "@types/mustache": "^4.2.2",
    "@types/node": "^20.5.9",
    "@types/nodemailer": "^6.4.9",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "prisma": "5.3.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "notifications",
    "erp",
    "nexus",
    "typescript",
    "bull",
    "websockets"
  ],
  "author": "ERP Nexus Team",
  "license": "ISC"
}
