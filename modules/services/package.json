{
  "name": "@nexus-erp/services",
  "version": "1.0.0",
  "description": "Services module for Nexus ERP - Professional Services Management system",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "node scripts/test-integration.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "nexus-erp",
    "services",
    "professionals",
    "appointments",
    "express",
    "prisma",
    "typescript",
    "redis",
    "multer"
  ],
  "author": "Nexus ERP Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "@prisma/client": "^5.6.0",
    "prisma": "^5.6.0",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.22.4",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "redis": "^4.6.10",
    "uuid": "^9.0.1",
    "axios": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "fs-extra": "^11.2.0",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.8.10",
    "@types/uuid": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "@types/multer": "^1.4.11",
    "@types/fs-extra": "^11.0.4",
    "@types/mime-types": "^2.1.4",
    "typescript": "^5.2.2",
    "tsx": "^4.1.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "eslint": "^8.53.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "prettier": "^3.0.3",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  }
}